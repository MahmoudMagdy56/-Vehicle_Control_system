/*(3) Write a C function that use the bubble sort algorithm to sort
an integer array in ascending order (search for the bubble sorting
algorithm).*/

#include <stdio.h>
struct employee {
	char name1[40];
	char name2[30];
	int age;
	int salary;
};

int main (void)
{
	setvbuf(stdout, NULL, _IONBF, 0);
	setvbuf(stderr, NULL, _IONBF, 0);
	struct employee emp1,*emp_ptr;
	emp_ptr=&emp1;
	strcpy((*emp_ptr).name1,"Mahmoud");
	strcpy(emp1.name2,"Magdy");
    (*emp_ptr).age=21;
    emp1.salary=21000;
    printf("%d %s %s %s",(*emp_ptr).age,emp1.name1,emp1.name2,emp1.name1);
	return 0;
}
